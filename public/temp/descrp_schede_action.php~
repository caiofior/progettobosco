<?php

if( isset($_GET['modify_cat']) ) {
	$codice 	= $_GET['modify_cat'] ;
	$particella	= $_GET['modify_cat2'];
	$cat		= $_GET['modify'];
	
	list( $res , $id ) = modifyInfoSchedeCatasto($cat, $codice, $particella) ;
	
	if( $res ) 	echo "modifica avvenuta con successo ( codice $codice e particella $particella )" ;
	else 		echo "errore" ;
}
?>